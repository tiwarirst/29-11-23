#include <stdio.h>

int main()
{
    int year;
    printf("enter year");
    scanf("%d",&year);
    if(year%100==100)
    {
        if (year%400==0)
        printf("leap year\n");
        else
        printf("not leap\n");
    }
    else
    {
        if(year%4==0)
        printf("leap year\n");
        else
        printf("not leap\n");
    }

    return 0;
}
